<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <sectionGroup name="yuyu.mvc" type="YuYu.Components.YuYuMvcConfigurationSectionGroup, YuYu.Extensions.ForMvc">
      <section name="staticizeRouteCollection" type="YuYu.Components.YuYuStaticizeRouteCollectionConfigurationSection, YuYu.Staticize.ForMvc" requirePermission="false" />
      
      
    <section name="globalFilterCollection" type="YuYu.Components.YuYuMvcGlobalFilterCollectionConfigurationSection, YuYu.Extensions.ForMvc" requirePermission="false" /><section name="routeCollection" type="YuYu.Components.YuYuMvcRouteCollectionConfigurationSection, YuYu.Extensions.ForMvc" requirePermission="true" /></sectionGroup>
  </configSections>
  <yuyu.mvc>
    <staticizeRouteCollection>
      <routes>
        <route name="Index" url="index.html">
          <defaults>
            <property name="controller" value="Home" />
            <property name="action" value="Index" />
          </defaults>
        </route>
      </routes>
    </staticizeRouteCollection>
    
    
  <globalFilterCollection>
      <filters>
        <filter type="System.Web.Mvc.HandleErrorAttribute, System.Web.Mvc" order="0" />
      </filters>
    </globalFilterCollection><routeCollection>
      <routes>
        <route name="Default" url="{controller}/{action}/{id}">
          <defaults>
            <property name="controller" value="Home" />
            <property name="action" value="Index" />
            <property name="id" value="UrlParameter.Optional" />
          </defaults>
        </route>
      </routes>
    </routeCollection></yuyu.mvc>
  
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="YuYu.Extensions" publicKeyToken="9623a3912ff6de59" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-4.5.0.1" newVersion="4.5.0.1" />
      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="System.Web.Mvc" publicKeyToken="31bf3856ad364e35" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-5.1.0.0" newVersion="5.1.0.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
<startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup></configuration>
