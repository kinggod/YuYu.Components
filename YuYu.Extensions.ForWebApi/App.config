<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <sectionGroup name="yuyu.webApi" type="YuYu.Components.YuYuWebApiConfigurationSectionGroup, YuYu.Extensions.ForWebApi">
      <section name="httpConfiguration" type="YuYu.Components.YuYuWebApiHttpConfigurationConfigurationSection, YuYu.Extensions.ForWebApi" requirePermission="false" />
    </sectionGroup>
  </configSections>
  <yuyu.webApi>
    <httpConfiguration>
      <webApis>
        <webApi name="DefaultApi" url="api/{controller}/{id}">
          <defaults>
            <property name="id" value="UrlParameter.Optional" />
          </defaults>
        </webApi>
      </webApis>
    </httpConfiguration>
  </yuyu.webApi>
<startup><supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" /></startup>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="Newtonsoft.Json" publicKeyToken="30ad4fe6b2a6aeed" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-6.0.0.0" newVersion="6.0.0.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
</configuration>
